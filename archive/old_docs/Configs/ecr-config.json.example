# Example ECR Configuration
# Copy this to ecr-config.json and fill with your actual values

{
    "registryUrl": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com",
    "region": "ap-southeast-1",
    "accountId": "<YOUR_AWS_ACCOUNT_ID>",
    "loginCommand": "aws ecr get-login-password --region <REGION> | docker login --username AWS --password-stdin <YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com",
    "repositories": {
        "gateway": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-gateway",
        "auth": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-auth",
        "user": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-user",
        "quiz": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-quiz",
        "game": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-game",
        "analytics": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-analytics",
        "frontend": "<YOUR_AWS_ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/kahoot-clone-frontend"
    }
}
